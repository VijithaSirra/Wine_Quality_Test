        });

    });


    function interstitialCloseIt()
    {
        $(".interstitial-container-popup").hide();
        $('.wrapper,.AdHabillage').addClass('opacity').show();
        clearInterval(interstitialTimer1);

        $('#endorsement-bar').css({"width":"100%","height":"28px"});
        if(getCookie('cookiesaccepted')!=1) {

            $('#cookie-notice').css({"display": "inline-block"});
            $('#cookie-notice').css({"visibility": "visible"});
        }
    }
    function interstitialOpenIt()
    {
        $('.wrapper,.AdHabillage').addClass('opacity');
        $(".interstitial-container-popup").show();
    }
    var countDownDate = new Date();
    countDownDate.setSeconds(countDownDate.getSeconds() + 15);

    // Update the count down every 1 second
    interstitialTimer1= setInterval(function() {

        // Get todays date and time
        var now = new Date().getTime();

        // Find the distance between now an the count down date
        var distance = countDownDate - now;

        // Time calculations for days, hours, minutes and seconds
        var seconds = Math.floor((distance % (1000 * 60)) / 1000);

        // Output the result in an element with id="countdown"
        document.getElementById("interstitial_countdown").innerHTML = seconds + "s ";

        // If the count down is over, write some text
        if (distance < 0) {
            interstitialCloseIt();
            document.getElementById("interstitial_countdown").innerHTML = "0";
        }
    }, 1000);

